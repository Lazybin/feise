{
    "basePath": "http://localhost/feise/public",
    "swaggerVersion": "1.2",
    "apiVersion": "0.2",
    "resourcePath": "/test",
    "apis": [
        {
            "path": "/test/index",
            "description": "Operations about pets",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Find pet by ID",
                    "nickname": "getPetById",
                    "type": "Pet",
                    "parameters": [
                        {
                            "paramType": "path",
                            "name": "petId",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "ID of pet that needs to be fetched"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 404,
                            "message": "Pet not found"
                        }
                    ],
                    "notes": "Returns a pet based on ID"
                }
            ]
        }
    ]
}